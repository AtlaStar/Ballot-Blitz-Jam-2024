config_version=5

[application]

config/name="Ballot Blitz Jam"
run/main_scene="res://area_3d.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

EventBus="*res://EventBus.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"Bill": "res://puppet pieces/gal/Bill.dch",
"Mayor Nem A Tode": "res://Mayor Nem A Tode.dch",
"Player": "res://Player.dch",
"Susan": "res://Susan.dch"
}
directories/dtl_directory={
"Bill_timeline": "res://puppet pieces/timelines/Bill_timeline.dtl",
"Mayor Nem A Tode_timeline": "res://Mayor Nem A Tode_timeline.dtl",
"Susan_timeline": "res://puppet pieces/timelines/Susan_timeline.dtl",
"Timeline Root": "res://Timeline Root.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=["res://speech_bubble_test.tres"]
layout/default_style=""
variables={
"carol_voting": false,
"demure_val": 0,
"met_bill": false,
"met_mayor": false,
"met_susan": false,
"snarky_val": 0,
"susan_stat_changed": false,
"susan_voting": false,
"talked_to_mayor_count": 0,
"troll_val": 0
}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}

[dotnet]

project/assembly_name="Ballot Blitz Jam"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

